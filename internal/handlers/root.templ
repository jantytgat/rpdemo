package handlers

import (
 "time"
 )

templ rootHandler(keys []string, headers map[string]string, stamp time.Time) {
    <h1>{ stamp.Format("02-01-2006 15:04:05") }</h1>
    <table>
    <thead>
        <tr>
            <td>Header</td>
            <td>Value</td>
        </tr>
    </thead>
    <tbody>
       for _, key := range keys {
            <tr>
                <td>{ key }</td>
                <td> { headers[key] } </td>
            </tr>
       }
    </tbody>
    </table>
}